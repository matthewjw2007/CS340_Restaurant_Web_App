<div class="container">
  <div class="col text-center mx-auto">
    <h1>List of Current Users</h1>
      <table class='style-table' id="users">
          <thead>
              <tr>
                  <th>Username</th>
                  <th>Actions</th>
              </tr>
          </thead>
          <tbody>
              {{#each users}}
              <tr id = {{usr_id}}>
                  <td>{{usr_name}}</td>
                  <td>
                      <button id="view_lists" type="submit" onclick="show_lists({{usr_id}})" class="btn btn-info horizontal-btn">View Lists
                      </button>
                      <button id="delBtn" type="submit" onclick="" class="btn btn-info horizontal-btn">Edit
                      </button>
                  </td>
              </tr>
              {{/each}}
          </tbody>
      </table>
  </div>
</div>